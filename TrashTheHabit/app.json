{
  "expo": {
    "name": "TrashTheHabit",
    "slug": "TrashTheHabit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/TTH.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-ico.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash-ico.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "splash": {
        "image": "./assets/splash-ico.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favico.png"
    },
    "platforms": ["ios", "android"],
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
